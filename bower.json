{
  "name": "dystill-web",
  "description": "A web front end for the dystill MDA.",
  "main": "",
  "authors": [
    "Rob Peck <rob@robpeck.com>"
  ],
  "license": "MIT",
  "moduleType": [],
  "homepage": "http://www.dystill.org",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "vendor/bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap-sass": "^3.3.6",
    "angular": "^1.5.0",
    "angular-bootstrap": "^1.1.2",
    "angular-resource": "^1.5.0",
    "font-awesome": "^4.6.3"
  },
  "overrides": {
      "font-awesome": {
        "main": [
          "less/font-awesome.less",
          "scss/font-awesome.scss",
          "fonts/*"
        ]
      },
      "bootstrap-sass": {
        "main": [
          "assets/stylesheets/_bootstrap.scss",
          "assets/javascripts/bootstrap.js",
          "assets/fonts/bootstrap/*"
        ]
      }
  }
}
